<% content_for :script do %>
  <%= javascript_include_tag 'AdminLTE_flot', 'data-turbolinks-track': 'reload' %>
<% end %>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1> Flot Charts <small>preview sample</small></h1>
    <ol class="breadcrumb">
      <li>
        <a href="#"><i class="fa fa-dashboard"></i> Home</a>
      </li>
      <li>
        <a href="#">Charts</a>
      </li>
      <li class="active">
        Flot
      </li>
    </ol>
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="row">
      <div class="col-xs-12">
        <!-- interactive chart -->
        <%= render partial: "admin_lte_test/index/content_wrapper_flot/interactive_chart", locals: { chart_no: 1 } %>
      </div>
      <!-- /.col -->
    </div>
    <!-- /.row -->

    <div class="row">
      <div class="col-md-6">
        <!-- Line chart -->
        <%= render partial: "admin_lte_test/index/content_wrapper_flot/line_chart" %>
        <!-- Area chart -->
        <%= render partial: "admin_lte_test/index/content_wrapper_flot/area_chart" %>
      </div>
      <!-- /.col -->

      <div class="col-md-6">
        <!-- Bar chart -->
        <%= render partial: "admin_lte_test/index/content_wrapper_flot/bar_chart" %>

        <!-- Donut chart -->
        <%= render partial: "admin_lte_test/index/content_wrapper_flot/donut_chart" %>
      </div>
      <!-- /.col -->
    </div>
    <!-- /.row -->
  </section>
  <!-- /.content -->

</div>
<!-- /.content-wrapper -->