<!-- layout/application -->
<!DOCTYPE html>
<html>
  <head>
    <%= render partial: "layouts/head_item" %>
    <title>EventManager</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'gmKB5', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
  </body>
</html>
